{	"description": "Photo Booth Configuration",
	"debug": true,
	"configuration": {
		"name": "Photobooth",
		"outputFolderPath": "/output",
		"IPaddress": "192.168.1.130",
		"mirrorScreen": false,
		"countDownStart": 3,
		"fileType": "jpg",
		"eventText": "Your Photo Booth Event",
		"instructionLineText": "^Look Here^",
	},
	"display": {
		"width": 3840,
		"height": 2160,
	},
    "camera": {
    	"name": "Logitech BRIO", 
		"numberDescription": "number index variation for the camera being used, 0 relative first",
		"number": 1,
        "width": 3840,
		"height": 2160,
		"fps": 30,
		"orientation": "landscape",
		"pipeline": "pipeline: ksvideosrc device-index=0 ! image/jpeg,width=3840,height=2160,framerate=30/1 ! jpegdec ! videoconvert",
    },
	"printer": {
		"name": "Canon Selphy 1300",
		"printWidth": 4.0,
		"printHeight": 6.0,
	}
}
